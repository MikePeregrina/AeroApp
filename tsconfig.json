{
  "extends": "expo/tsconfig.base",
  "skipLibCheck": true,
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts", 
    "app/index.tsx", "screen/Welcome.tsx", "components/Logo.tsx", "components/Carrousel.jsx", "components/data.ts", "components/CarouselCardItem.tsx", "screen/DetailsScreen.jsx"  ],
  "exclude": ["node_modules"]
}
