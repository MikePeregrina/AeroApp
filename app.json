{
  "expo": {
    "name": "aeromobile",
    "slug": "aeromobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.widolearn.widoapp"
    },
    "android": {
      "package": "com.widolearn.widoapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "generate-mapping",
          "config": {}
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/favicon.png",
      "disableReactNativeWarnings": true
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6d968f7d-28bc-4d37-ae11-fc0987fdc29f"
      }
    }
  }
}
